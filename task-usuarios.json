{
  "executionRoleArn": "arn:aws:iam::006078069859:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/task-eaesof-usuarios",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      
      "cpu": 0,
      "environment": [
        {
          "name": "MYSQL_IP",
          "value": "carroya-eaesof.cw8wuioquzwa.us-east-1.rds.amazonaws.com:usuarios"
        },
        {
          "name": "MYSQL_PORT",
          "value": "3306"
        },
        {
          "name": "MYSQL_USER",
          "value": "carroya"
        },
        {
          "name": "MYSQL_PWD",
          "value": "CarroyaRecargado"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "image": "006078069859.dkr.ecr.us-east-1.amazonaws.com/usuarios-eaesof:usuarios",
	  "dependsOn": [
        {
          "containerName": "envoy",
          "condition": "HEALTHY"
        }
      ],
      "essential": true,
      "name": "cont-eaesof-usuarios"
    },
    {
      "portMappings": [],
       "cpu": 0,
      "environment": [
        {
          "name": "APPMESH_VIRTUAL_NODE_NAME",
          "value": "mesh/eaesof/virtualNode/usuarios"
          
        }
      ],
      "mountPoints": [],
      "memory": 500,
      "volumesFrom": [],
      "image": "840364872350.dkr.ecr.us-east-1.amazonaws.com/aws-appmesh-envoy:v1.23.1.0-prod",
        "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/task-eaesof-usuarios",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
      "healthCheck": {
        "retries": 3,
        "command": [
          "CMD-SHELL",
          "curl -s http://localhost:9901/server_info | grep state | grep -q LIVE"
        ],
        "timeout": 2,
        "interval": 5,
        "startPeriod": 10
      },
      "essential": true,
      "user": "1337",
      "name": "envoy"
    }
  ],
  "placementConstraints": [],
  "memory": "1024",
  "taskRoleArn": "arn:aws:iam::006078069859:role/ecsTaskExecutionRole",

  "family": "task-eaesof-usuarios",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "proxyConfiguration": {
    "type": "APPMESH",
    "containerName": "envoy",
    "properties": [
      {
        "name": "ProxyIngressPort",
        "value": "15000"
      },
      {
        "name": "AppPorts",
        "value": "8080"
      },
      {
        "name": "EgressIgnoredIPs",
        "value": "169.254.170.2,169.254.169.254"
      },
      {
        "name": "IgnoredGID",
        "value": ""
      },
      {
        "name": "EgressIgnoredPorts",
        "value": "8899,27017"
      },
      {
        "name": "IgnoredUID",
        "value": "1337"
      },
      {
        "name": "ProxyEgressPort",
        "value": "15001"
      }
    ]
  },
  "volumes": []
}
